<div class="video-component-wrapper___2BSGde">
  <div class="enargy_video_one">
    <video width="100%" height="100%" autoplay muted loop playsinline class="video-main-class" src="{{ section.settings.video_ENERGY_url }}" type="video/mp4">
    </video>
  </div>
  <div class="enargy_video_mobile">
    <video width="100%" height="100%" autoplay muted loop playsinline class="video-main-class" src="{{ section.settings.energy_video_mobile }}" type="video/mp4">
    </video>
  </div>
  <div class="energy-video-text-conten">
    <div class="poster_conten">
        <img src="{{section.settings.grow_img | img_url: 'master'}}">
        <h2 style="color:{{section.settings.head_I1}}; font-family:{{section.settings.head_I2}}; font-size:{{section.settings.head_I3}}px;" class="video-title new_g5_terminal">{{ section.settings.video_ENERGY_title }}</h2>
        <p style="color:{{section.settings.head_I4}}; font-family:{{section.settings.head_I5}}; font-size:{{section.settings.head_I6}}px;" class="enargy_sometext">{{ section.settings.video_ENERGY_text }}</p>
        <a style="color:{{section.settings.head_I7}}; font-family:{{section.settings.head_I8}}; font-size:{{section.settings.head_I9}}px;" class="enargy-btn" href="{{ section.settings.video_ENERGY_btn_url }}">{{ section.settings.video_ENERGY_btn_text }}</a>
    </div>
  </div>

</div>


